# BTC Lending Platform Frontend Environment Variables
# Copy this file to .env and update the values

# Contract Addresses (Update these after deployment)
REACT_APP_WALLET_ADDRESS=YOUR_WALLET_ADDRESS
REACT_APP_INTEREST_RATE_MODEL=YOUR_WALLET_ADDRESS::interest_rate_model
REACT_APP_COLLATERAL_VAULT=YOUR_WALLET_ADDRESS::collateral_vault
REACT_APP_LOAN_MANAGER=YOUR_WALLET_ADDRESS::loan_manager
REACT_APP_CTRL_BTC_TOKEN=YOUR_WALLET_ADDRESS::ctrl_btc_token
REACT_APP_LNBTC_TOKEN=YOUR_WALLET_ADDRESS::ln_btc_token

# Network Configuration
REACT_APP_NETWORK=testnet
REACT_APP_REST_URL=https://fullnode.testnet.aptoslabs.com/v1
REACT_APP_EXPLORER_URL=https://explorer.testnet.aptoslabs.com

# App Configuration
REACT_APP_NAME=BTC Lending Platform
REACT_APP_VERSION=1.0.0

